<?xml version="1.0" encoding="UTF-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:lang="clr-namespace:SundihomeApp.Resources" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:controls="clr-namespace:SundihomeApp.Controls" x:Class="SundihomeApp.Views.LookUpPage">
    <ContentView.Content>
        <Grid x:Name="MainGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <controls:SearchBarFrame Grid.Row="0" Margin="5,5,5,0">
                <controls:SearchBar x:Name="searchBar" Placeholder="{x:Static lang:Language.tim_kiem}" />
            </controls:SearchBarFrame>
        </Grid>
    </ContentView.Content>
</ContentView>
